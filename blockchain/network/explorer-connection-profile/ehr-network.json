{
    "name": "ehr-network",
    "version": "1.0.0",
    "client": {
        "tlsEnable": true,
        "adminCredential": {
            "id": "admin",
            "password": "adminpw"
        },
        "enableAuthentication": false,
        "organization": "HospitalMSP",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                },
                "orderer": "300"
            }
        }
    },
    "channels": {
        "ehr-channel": {
            "peers": {
                "peer0.hospital.ehr.com": {},
                "peer1.hospital.ehr.com": {},
                "peer0.clinic.ehr.com": {},
                "peer0.lab.ehr.com": {}
            }
        }
    },
    "organizations": {
        "HospitalMSP": {
            "mspid": "HospitalMSP",
            "adminPrivateKey": {
                "path": "/tmp/crypto/hospital/users/Admin@hospital.ehr.com/msp/keystore"
            },
            "peers": [
                "peer0.hospital.ehr.com",
                "peer1.hospital.ehr.com"
            ],
            "signedCert": {
                "path": "/tmp/crypto/hospital/users/Admin@hospital.ehr.com/msp/signcerts"
            }
        },
        "ClinicMSP": {
            "mspid": "ClinicMSP",
            "adminPrivateKey": {
                "path": "/tmp/crypto/clinic/users/Admin@clinic.ehr.com/msp/keystore"
            },
            "peers": [
                "peer0.clinic.ehr.com"
            ],
            "signedCert": {
                "path": "/tmp/crypto/clinic/users/Admin@clinic.ehr.com/msp/signcerts"
            }
        },
        "LabMSP": {
            "mspid": "LabMSP",
            "adminPrivateKey": {
                "path": "/tmp/crypto/lab/users/Admin@lab.ehr.com/msp/keystore"
            },
            "peers": [
                "peer0.lab.ehr.com"
            ],
            "signedCert": {
                "path": "/tmp/crypto/lab/users/Admin@lab.ehr.com/msp/signcerts"
            }
        }
    },
    "peers": {
        "peer0.hospital.ehr.com": {
            "tlsCACerts": {
                "path": "/tmp/crypto/hospital/peers/peer0.hospital.ehr.com/tls/ca.crt"
            },
            "url": "grpcs://peer0.hospital.ehr.com:7051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.hospital.ehr.com"
            }
        },
        "peer1.hospital.ehr.com": {
            "tlsCACerts": {
                "path": "/tmp/crypto/hospital/peers/peer1.hospital.ehr.com/tls/ca.crt"
            },
            "url": "grpcs://peer1.hospital.ehr.com:8051",
            "grpcOptions": {
                "ssl-target-name-override": "peer1.hospital.ehr.com"
            }
        },
        "peer0.clinic.ehr.com": {
            "tlsCACerts": {
                "path": "/tmp/crypto/clinic/peers/peer0.clinic.ehr.com/tls/ca.crt"
            },
            "url": "grpcs://peer0.clinic.ehr.com:9051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.clinic.ehr.com"
            }
        },
        "peer0.lab.ehr.com": {
            "tlsCACerts": {
                "path": "/tmp/crypto/lab/peers/peer0.lab.ehr.com/tls/ca.crt"
            },
            "url": "grpcs://peer0.lab.ehr.com:10051",
            "grpcOptions": {
                "ssl-target-name-override": "peer0.lab.ehr.com"
            }
        }
    }
}